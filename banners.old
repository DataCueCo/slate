    <span class="s2">"desktop"</span> <span class="o">=&gt;</span> <span class="s2">"http://s3.amazon.com/photoDesktop.png"</span>
  <span class="p">),</span>
  <span class="s2">"link"</span> <span class="o">=&gt;</span> <span class="s2">"path/to/anything"</span><span class="p">,</span>
